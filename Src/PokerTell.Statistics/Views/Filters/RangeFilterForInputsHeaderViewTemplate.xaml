<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <DataTemplate x:Key="RangeFilterForInputsHeaderViewTemplate">
        <StackPanel Orientation="Horizontal">
            <CheckBox IsChecked="{Binding IsActive}"/>
            <Label Content="{Binding FilterName}" Margin="0, 0, 5, 0"/>
            <Label Content="{Binding MinValue, Mode=OneWay}" Margin="0, 0, 5, 0"/>
            <Label Content="to" Margin="0, 0, 5, 0"/>
            <Label Content="{Binding MaxValue, Mode=OneWay}" Margin="0, 0, 5, 0"/>
        </StackPanel>
    </DataTemplate>

</ResourceDictionary>